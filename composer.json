{
    "name": "isla-transfers/isla-transfers-app",
    "description": "Aplicación de reserva y gestión de transfers.",
    "type": "project",
    "license": "proprietary",
    "authors": [
        {
            "name": "Pol Valle",
            "email": "doplax@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "phpmailer/phpmailer": "^6.9",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\Controllers\\": "app/controllers/",
            "App\\Controllers\\Api\\": "app/controllers/api/",
            "App\\Models\\": "app/models/",
            "App\\Helpers\\": "app/helpers/",
            "App\\Core\\": "app/core/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
